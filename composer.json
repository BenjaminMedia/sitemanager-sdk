{
    "name": "bonnier/sitemanager-sdk",
    "description": "A PHP SDK for SiteManager",
    "type": "library",
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "illuminate/support": "^5.5",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "fzaninotto/faker": "^1.8"
    },
    "authors": [
        {
            "name": "Bonnier Publications",
            "email": "interactive@bonnier.dk"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bonnier\\SiteManager\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bonnier\\SiteManager\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox",
        "unit": "vendor/bin/phpunit --testsuite=Unit Tests --testdox"
    },
    "archive": {
        "exclude": ["/.circleci", "/tests", "/phpunit.xml"]
    }
}
